@model IEnumerable<WebAdminConsole.ViewModels.MountainResultsViewModel>


@{
    ViewData["Title"] = "MountainResults";
}

<h1>MountainResults</h1>

<table class="table">

    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Position)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.TeamName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Stage4)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Stage5)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Stage16)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Stage18)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.TotalTime)
            </th>
        </tr>
    </thead>

    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.Position)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.TeamName)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Stage4)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Stage5)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Stage16)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Stage18)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.TotalTime)
                </td>
            </tr>
        }
    </tbody>

</table>
